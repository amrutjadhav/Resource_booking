<%= form_for ['admin', @employee] do | f |%>
<div class="row">
<div class="col-md-6 col-md-offset-3">
		<%=f.label :Name%>
	
		<%=f.text_field :name %>
		<%=f.label :Email%>
		<%=f.text_field :email %>
		<%=f.label :Age%>
		<%=f.text_field :age %>
		<%=f.label :DateOfJoining %>
		<%=f.date_field :date_of_joining %>
		<%=f.label :ManagerId%>
		<%=f.text_field :manager_id %>
		<%=f.label :RoleId%>
		<%=f.text_field :role_id %>
		<%= f.submit 'Save Employee'%>
<% end %>
