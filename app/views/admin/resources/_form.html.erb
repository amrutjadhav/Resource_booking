<%= render "error" %>

<div class="row">
 	<div class="col-md-6 col-md-offset-3" >
	<%= form_for ["admin",@resource] do | f |%>
		<%=f.label :Name%></td>
		<%=f.text_field :name , required: true, class: "form-control"%>
		<%=f.label :Count%>
		<%=f.text_field :count , class: "form-control"%>
		<%=f.label :Timeslot%>
		<%=f.time_field :time_slot , required: true%>
	<%= f.submit 'Save Resource',class: "btn btn-success" %>
	<% end %>
</div>
</div>
