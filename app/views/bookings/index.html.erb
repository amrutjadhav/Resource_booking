
<div class="col-md-12">
	<div class="col-md-1">
	<strong>Status</strong>
	</div>
	<div class="col-md-1">
	<strong>Priority</strong>
	</div>
	<div class="col-md-2">
	<strong>Feedback</strong>
	</div>
	<div class="col-md-2">
	<strong>Comment</strong>
	</div>
	<div class="col-md-2">
 	<strong>Shift</strong>
 	</div>
 	<div class="col-md-2">
	<strong>Employee_id</strong>
	</div>
</div>
<div class="col-md-12">
	
<% @bookings.each do |booking|%>
	<div class="col-md-1">
	<%=status_normalization(booking.status)%>
	</div>
	<div class="col-md-1">
	<%=priority_normalization(booking.priority)%>
	</div>
	<div class="col-md-2	">
	<%=booking.feedback%>
	</div>
	<div class="col-md-2">
	<%=booking.comment%>
	</div>
	<div class="col-md-2">
	<%=booking.shift%>
	</div>
	<div class="col-md-2">
	<%=booking.employee_id%>
	</div>
</div>
<% end %>
</body>	